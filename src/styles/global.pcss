/* Base global styles */
body {
  font-family: "Overpass", Arial, sans-serif;
}

/* Crow Nest global font utilities */
.crow-nest-title {
  font-family: "Eveleth", "Overpass", Arial, sans-serif;
  font-weight: bold;
  text-transform: uppercase;
  letter-spacing: 1px;
}

.crow-nest-subtitle {
  font-family: "Eveleth Slant", "Eveleth", "Overpass", Arial, sans-serif;
}

.crow-nest-body {
  font-family: "Overpass", Arial, sans-serif;
}
